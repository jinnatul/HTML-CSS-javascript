<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Get Channel List</title>
</head>
<body>
  <h1>Show console</h1>
  <script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
  <script>
    $(function() {
  $.get('https://iptv-org.github.io/iptv/channels.json', function(res) {
    let data = [];
    for (let i = 0; i < res.length; i++) {
      if (res[i].category === "Sport") {
        data.push({ 
          "name": res[i].name, 
          "logo": res[i].logo,
          "url": res[i].url,
          "country": res[i].country.name,
          "category": res[i].category
        });
      }
    }
    console.log(data)
    data = JSON.stringify(data);
    $.post("http://localhost:3000/data", {data: data}, function(res1) {
      console.log(res1)
    });
  })
})
  </script>
</body>
</html>